<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 3.2//EN"
        "http://tuckey.org/res/dtds/urlrewrite3.2.dtd">

<urlrewrite>
   <rule>
       <note>将所有.action文件映射为.html</note>
        <!--<from>(.*).html</from>-->  
        <from>(.*).html;jsessionid=.*$</from>
        <to>$1.action</to>  
    </rule>
     <rule>
       <note>将所有.action文件映射为.html</note>
        <from>(.*).html</from>
        <to>$1.action</to>  
    </rule>
</urlrewrite>


