<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="admin-systemsetting" namespace="/admin/system"
		extends="wzd-admin">
		<action name="index" class="SystemConfigAction" method="index">
			<result name="success" type="ftl">/admin/system/index.html
			</result>
		</action>
		<action name="update" class="SystemConfigAction" method="update">
			<result name="success" type="ftl">/admin/system/index.html
			</result>
		</action>
		
		<action name="welcome" class="SystemConfigAction" method="welcome">
			<result name="success" type="ftl">/admin/admin.html
			</result>
		</action>
		
		<action name="clean" class="SystemConfigAction" method="clean">
			<result name="success" type="redirect">/admin/system/index.html
			</result>
		</action>
		
		<action name="add" class="SystemConfigAction" method="add">
			<result name="success" type="ftl">/admin/system/addSysConfig.html
			</result>
		</action>
	</package>
</struts>
